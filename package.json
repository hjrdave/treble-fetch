{
  "name": "treble-fetch",
  "version": "1.0.0-beta.15",
  "description": "Fetching, Prefetching, and Cache Extension for Treble Global State Management Library",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "babel --watch src --out-dir lib --extensions .ts,.tsx",
    "build": "babel src --out-dir lib --extensions .ts,.tsx && tsc --emitDeclarationOnly",
    "tsc:watch": "tsc --watch --emitDeclarationOnly"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE",
    "/lib/index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hjrdave/treble-fetch.git"
  },
  "keywords": [
    "npm",
    "module",
    "node",
    "javascript",
    "typescript",
    "react",
    "react-hooks",
    "global-state-management",
    "fetching",
    "prefetching",
    "preloading"
  ],
  "author": "David Sanders <dsanders@mediafish.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hjrdave/treble-fetch/issues"
  },
  "homepage": "https://github.com/hjrdave/treble-fetch#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^13.13.4",
    "@types/uniqid": "^4.1.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.1.2",
    "cross-env": "^5.2.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "rimraf": "^2.6.2",
    "typescript": "^4.6.2",
